{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "blog_category": {
      "model": "blog-categories",
      "via": "blog_posts"
    },
    "blogDetails": {
      "type": "richtext"
    },
    "blog_tag": {
      "model": "blog-tag",
      "via": "blog_posts"
    }
  }
}
